{
    "name": "jenova13q/Test",
    "type": "library",
    "version": "1.0",
    "description": "test",
    "keywords": [
        "jenova13q",
        "Test"
    ],
    "homepage": "https://github.com/jenova13q/Test",
    "license": "MIT",
    "authors": [
        {
            "name": "KostyukKate",
            "email": "jenova13q@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jenova13q\\Test\\": "src",
            "jenova13q\\Test\\Http\\Controllers\\": "src/Http/Controllers/",
            "jenova13q\\Test\\Models\\": "src/Models/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jenova13q\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
